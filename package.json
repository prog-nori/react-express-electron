{
  "name": "react-express",
  "version": "0.0.0",
  "private": true,
  "main": "./electron.js",
  "homepage": "./",
  "scripts": {
    "__start": "node ./bin/www",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "electron-start": "wait-on http://localhost:3000 && electron .",
    "electron-build": "electron-builder",
    "client": "PORT=3002 npm start --prefix client",
    "dev": "concurrently \"npm run electron-start\" \"npm run server\" \"npm run client\" "
  },
  "dependencies": {
    "axios": "^0.25.0",
    "concurrently": "^7.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "electron-is-dev": "^2.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "npm-run-all": "^4.1.5",
    "wait-on": "^6.0.1"
  }
}
